{
    "author": "kai zhu <kaizhu256@gmail.com>",
    "buildNpmdoc": "tar.gz",
    "description": "#### basic api documentation for  [tar.gz (v1.0.5)](https://github.com/alanhoff/node-tar.gz#readme)  [![npm package](https://img.shields.io/npm/v/npmdoc-tar.gz.svg?style=flat-square)](https://www.npmjs.org/package/npmdoc-tar.gz) [![travis-ci.org build-status](https://api.travis-ci.org/npmdoc/node-npmdoc-tar.gz.svg)](https://travis-ci.org/npmdoc/node-npmdoc-tar.gz)",
    "devDependencies": {
        "electron-lite": "kaizhu256/node-electron-lite#alpha",
        "tar.gz": "*",
        "utility2": "kaizhu256/node-utility2#alpha"
    },
    "engines": {
        "node": ">=4.0"
    },
    "homepage": "https://github.com/npmdoc/node-npmdoc-tar.gz",
    "keywords": [
        "documentation",
        "tar.gz"
    ],
    "license": "MIT",
    "main": "lib.npmdoc_tar.gz.js",
    "name": "npmdoc-tar.gz",
    "nameAlias": "npmdoc_tar_gz",
    "nameOriginal": "npmdoc-tar.gz",
    "os": [
        "darwin",
        "linux"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/npmdoc/node-npmdoc-tar.gz"
    },
    "scripts": {
        "build-ci": "utility2 shReadmeTest build_ci.sh",
        "env": "env",
        "heroku-postbuild": "(set -e; npm install \"kaizhu256/node-utility2#alpha\"; utility2 shDeployHeroku)",
        "postinstall": "if [ -f npm_scripts.sh ]; then ./npm_scripts.sh postinstall; fi",
        "start": "(set -e; export PORT=${PORT:-8080}; utility2 start test.js)",
        "test": "(set -e; export PORT=$(utility2 shServerPortRandom); utility2 test test.js)"
    },
    "version": "2017.4.26"
}
